package com.hcl.enu;

class First {
	public void show() {
		System.out.println("Show method  from class first ");
	}
}

class Second extends First {
	public void show() {
		super.show();
		System.out.println("Show method  from class second");
	}
}

public class InhDemo {

	public static void main(String[] args) {
		Second obj=new Second();
		obj.show();
	}
}
