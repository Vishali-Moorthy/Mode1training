package com.hcl.quiz;

public class Quiz5 {
	
	public void calc(double r) {
		double c;
		c=Math.PI*r;
		System.out.println(c);
	}
	public void power(double r) {
		double a;
		a=Math.pow(r, 2);
		System.out.println(a);
	}
	public static void main(String[] args) {
		double r=12.3;
		new Quiz5().calc(r);
		new Quiz5().power(r);
		
	}

}
