package com.hcl.main;

class Point {
	
	protected int x, y; 
	   
    public Point(int a_x, int a_y) { 
        x = a_x; 
        y = a_y; 
    } 
} 
  
public class Main { 
    public static void main(String args[]) { 
      Point p = new Point();
      System.out.println("x = " + p.x + ", y = " + p.y); 
    }     
} 


