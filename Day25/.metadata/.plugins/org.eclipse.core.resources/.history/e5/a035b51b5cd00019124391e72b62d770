package com.hcl.main;

public class Main {
	
	protected int x, y; 
	   
    public Main(int _x, int _y) { 
        x = _x; 
        y = _y; 
    } 
} 
  
public class Main { 
    public static void main(String args[]) { 
      Point p = new Point(); 
      System.out.println("x = " + p.x + ", y = " + p.y); 
    }     
} 

}
