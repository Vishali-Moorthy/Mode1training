package com.hcl.collections;

interface IHello {
	void sayHello();
}
public class Lambda2 {
	public static void main(String[] args) {
		IHello h1 = () -> {
			System.out.println("Hello from Rishab");
		};
		IHello h2 = () -> {
			System.out.println("Hello from lakshmi");
		};
		IHello h3 = () -> {
			System.out.println("Hello from Laksh");
		};
		h1.sayHello();
		h2.sayHello();
		h3.sayHello();
	}

}
