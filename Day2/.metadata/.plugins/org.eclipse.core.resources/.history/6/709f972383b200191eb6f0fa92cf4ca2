package com.hcl.boxing;

public class BoxStudent {
	
	public void show(Object ob) {
		Student s=(Object)ob;
		System.out.println(s);
	}
	public static void main(String[] args) {
		
		Student objStud=new Student();
		int sno=46;
		String name="Visha";
		String city="Tirupur";
		double cgp=8.5;
		
		new BoxStudent().show(objStud);
	}

}
