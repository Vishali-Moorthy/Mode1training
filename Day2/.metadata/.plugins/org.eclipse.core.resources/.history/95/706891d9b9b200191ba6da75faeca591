package com.hcl.inh;

class A {
public void display() {
	System.out.println("Hello");
}
private void show(int a) {
	System.out.println("Value: " +a);
	
}
}
class B extends A {
	protected void disp() {
		System.out.println("Hi");
	}
}
public class AccessMod1 {
	public static void main(String[] args) {
		
		B ob=new B();
		ob.display();
		ob.disp();
		
	}
}
	
